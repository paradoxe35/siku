        "dropzone": "^5.7.2",
        "emailjs-com": "^2.6.3",
        "i18next": "^19.7.0",
        "intersection-observer": "^0.11.0",
        "jquery": "^3.5.1",
        "lozad": "^1.15.0",
        "nprogress": "^0.2.0",
        "popper.js": "^1.16.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-i18next": "^11.7.0",
